# Dejafoo Environment Configuration
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_DEFAULT_REGION=us-east-1

# Dejafoo Configuration
DEJAFOO_ENVIRONMENT=dev
DEJAFOO_PROJECT_NAME=dejafoo-dev

# GitHub Configuration (for CodeBuild)
GITHUB_REPO_URL=https://github.com/yourusername/dejafoo.git

# Optional: Override default values
# DEJAFOO_ENVIRONMENT=prod
# DEJAFOO_PROJECT_NAME=my-company-dejafoo
